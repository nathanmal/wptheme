{
  "name": "wptheme",
  "version": "0.5.0",
  "description": "Bootstrap 4 Starter Theme for Wordpress",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "node-sass --source-comments assets/scss --output assets/css",
    "sass:watch": "nodemon -watch assets/scss -e scss -x \"npm run sass\"",
    "dev": "webpack --config ./config/webpack.config.js --mode development",
    "dev:watch": "webpack --config ./config/webpack.config.js --mode development --watch",
    "build": "webpack --config ./config/webpack.config.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nathanmal/wptheme.git"
  },
  "keywords": [
    "wordpress",
    "theme",
    "bootstrap",
    "responsive"
  ],
  "author": "Nathan Malachowski",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/nathanmal/wptheme/issues"
  },
  "homepage": "https://github.com/nathanmal/wptheme#readme",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-css": "^4.2.1",
    "clean-css-loader": "^1.0.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "nodemon": "^1.14.8",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.4"
  }
}
